{
    "cSpell.words": [
        "addbooks",
        "biblos",
        "CURSORCLASS",
        "idauthor",
        "ideditorial",
        "idtitles",
        "mysecretkey",
        "mysqldb",
        "pymysql",
        "sqlalchemy"
    ],
    "sqltools.connections": [
        {
            "mysqlOptions": {
                "authProtocol": "default",
                "enableSsl": "Disabled"
            },
            "previewLimit": 50,
            "server": "localhost",
            "port": 3306,
            "driver": "MariaDB",
            "name": "desarrollo_flask",
            "database": "biblos12",
            "username": "root"
        }
    ],
    "python.testing.pytestArgs": [
        "app"
    ],
    "python.testing.unittestEnabled": false,
    "python.testing.pytestEnabled": true,
    "files.autoSave": "onWindowChange",
    "editor.inlineSuggest.syntaxHighlightingEnabled": true,
    "terminal.integrated.accessibleViewPreserveCursorPosition": true,
    "terminal.integrated.automationProfile.windows": {
        "PowerShell": {
            "source": "PowerShell",
            "args": [
                "-NoLogo",
                "-NoProfile",
                "-Command",
                "Invoke-Expression $(Get-Content -Path $env:USERPROFILE\\.vscode\\settings.json -Raw)"
            ]
        },
    }